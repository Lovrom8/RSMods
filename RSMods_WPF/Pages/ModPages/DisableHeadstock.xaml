<modpages:ModPage
    xmlns:modpages="clr-namespace:RSMods_WPF.Pages.ModPages"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    x:Class="RSMods_WPF.Pages.ModPages.DisableHeadstock"
    mc:Ignorable="d" 
    d:DesignHeight="450" d:DesignWidth="800"
    Title="DisableHeadstock"
>
    <Grid>
		<Button Content="&lt;" Name="GoBack" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0" Click="GoBack_Click"/>
		<Label x:Name="When_Label" Content="Remove Headstock When:" HorizontalAlignment="Left" Margin="63,80,0,0" VerticalAlignment="Top"/>
		<RadioButton x:Name="When_InSong" Content="In Song" HorizontalAlignment="Left" Margin="234,86,0,0" VerticalAlignment="Top" Checked="When_InSong_Checked"/>
        <RadioButton x:Name="When_Always" Content="Always" HorizontalAlignment="Left" Margin="313,86,0,0" VerticalAlignment="Top" Checked="When_Always_Checked"/>
    </Grid>
</modpages:ModPage>

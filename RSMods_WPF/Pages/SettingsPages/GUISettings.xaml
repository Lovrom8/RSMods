<settingsPages:SettingsPage
    x:Class="RSMods_WPF.Pages.SettingsPages.GUISettings"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:settingsPages="clr-namespace:RSMods_WPF.Pages.SettingsPages"
    mc:Ignorable="d" 
    d:DesignHeight="450" d:DesignWidth="800"
    Title="GUISettings"
>
    <Grid>
        <CheckBox Name="DarkMode" Content="Dark Mode" Foreground="{DynamicResource MaterialDesignBody}" HorizontalAlignment="Left" Margin="80,24,0,0" VerticalAlignment="Top" Checked="ChangeDarkMode" Unchecked="ChangeDarkMode"/>
        <Label Name="Credits" Content="Originally Created By:&#xD;&#xA;Ffio - Main GUI Dev&#xD;&#xA;LovroM8 - Set &amp; Forget&#xD;&#xA;ZagatoZee - Co-GUI Dev" HorizontalAlignment="Left" Margin="651,368,0,0" VerticalAlignment="Top" Width="139"/>
        <Button Name="ChangePrimaryColor" Content="Change Primary Color" HorizontalAlignment="Left" Margin="80,61,0,0" VerticalAlignment="Top" Click="ChangePrimaryColor_Click"/>
    </Grid>
</settingsPages:SettingsPage>
